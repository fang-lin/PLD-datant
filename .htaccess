RewriteEngine On

RewriteCond $1 !^(index\.php|robots\.txt|bower|dist|css|images|fonts|pictures|admin_images|icons|albums)
RewriteRule ^(.*)$ index.php/$1 [L]

ExpiresActive On
ExpiresByType text/html "access plus 0 months"
ExpiresByType text/css "access plus 0 months"
ExpiresByType image/gif "access plus 1 months"
ExpiresByType image/jpeg "access plus 1 months"
ExpiresByType image/jpg "access plus 1 months"
ExpiresByType image/png "access plus 1 months"
ExpiresByType application/x-shockwave-flash "access plus 0 months"
ExpiresByType application/javascript "access plus 0 months"
ExpiresByType application/x-javascript "access plus 0 months"
ExpiresByType video/x-flv "access plus 0 months"

